{
  "source": {
    "include": ["./config","./controllers","./models","./routes","./server","./services","./transformservice","./utils","./webscrapper"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./readme.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  }
}